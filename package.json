      "\\.(css|less|scss|sass|css?raw)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
